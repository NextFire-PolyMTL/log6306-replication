+          return {
+            result: result,
+            query: query,
+          };
+        })
+        .catch(err => {
+          throw {
+            error: err,
+            query: query,
+          };
+        });
+    });
+  }
+
+  annotationQuery(options) {}
+
+  metricFindQuery(query: string) {
+    const resourceGroupsQuery = query.match(/^ResourceGroups\(\)/i);
