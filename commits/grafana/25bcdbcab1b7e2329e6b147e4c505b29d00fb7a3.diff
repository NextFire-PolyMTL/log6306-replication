-const filterColumnKeys = key => key && key[0] !== '_' && key !== 'result' && key !== 'table';
-
-const IGNORE_FIELDS_FOR_NAME = ['result', '', 'table'];
-
-export const getTagsFromRecord = record =>
-  Object.keys(record)
-    .filter(key => key[0] !== '_')
-    .filter(key => IGNORE_FIELDS_FOR_NAME.indexOf(key) === -1)
